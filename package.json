{
  "name": "dbfuse-ai",
  "version": "1.3.1",
  "description": "A GUI application for mysql, postgreSQL, MSSQL ,oracledb and sqlite, which helps developers in faster development",
  "main": "index.js",
  "scripts": {
    "start": "nodemon src/index.js",
    "client": "cd client/dbfuse-ai-client && npm run start",
    "dev": "concurrently \"npm run start\" \"npm run client\"",
    "test": "npm run test:all",
    "test:all": "node src/config/tests/run-all-tests.js",
    "test:mysql": "node src/config/tests/mysql-test-suite.js",
    "test:postgres": "node src/config/tests/postgres-test-suite.js", 
    "test:oracle": "node src/config/tests/oracle-test-suite.js",
    "test:mssql": "node src/config/tests/mssql-test-suite.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kshashikumar/DBFuse-AI.git"
  },
  "author": "shashi kumar kasturi",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kshashikumar/DBFuse-AI/issues"
  },
  "homepage": "https://github.com/kshashikumar/DBFuse-AI#readme",
  "dependencies": {
    "@langchain/anthropic": "^0.3.25",
    "@langchain/cohere": "^0.3.4",
    "@langchain/community": "^0.3.49",
    "@langchain/google-genai": "^0.1.4",
    "@langchain/mistralai": "^0.2.1",
    "@langchain/openai": "^0.3.14",
    "axios": "^1.11.0",
    "base-64": "^1.0.0",
    "body-parser": "^1.20.3",
    "chalk": "^4.1.2",
    "connect-gzip-static": "^4.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "knex": "^3.1.0",
    "langchain": "^0.3.6",
    "minimist": "^1.2.8",
    "mssql": "^11.0.1",
    "mysql2": "^3.11.3",
    "nodemon": "^3.1.7",
    "oracledb": "^6.8.0",
    "pg": "^8.16.3",
    "sqlite3": "^5.1.7"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "bin": {
    "dbfuse-ai": "./cli.js"
  },
  "devDependencies": {
    "concurrently": "^9.1.0"
  }
}
